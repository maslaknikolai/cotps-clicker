    "url": "https://github.com/maslaknikolai/cotps-clicker/issues"
  },
  "homepage": "https://github.com/maslaknikolai/cotps-clicker#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0"
  }
